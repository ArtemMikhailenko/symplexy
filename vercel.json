{
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920],
    "domains": [],
    "minimumCacheTTL": 31536000,
    "formats": ["image/avif", "image/webp"]
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
